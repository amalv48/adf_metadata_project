{
	"name": "pl_TEST_email_lookup",
	"properties": {
		"activities": [
			{
				"name": "Get email addresses TEST",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.00:10:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "SELECT * FROM [dbo].[EmailRecipients]\nWHERE SystemCode = pipeline().parameters.systemCode AND DatasetName = pipeline().parameters.datasetName",
							"type": "Expression"
						},
						"queryTimeout": "00:10:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_asql_emailaddresses",
						"type": "DatasetReference"
					}
				}
			}
		],
		"parameters": {
			"systemCode": {
				"type": "string",
				"defaultValue": "OWS"
			},
			"datasetName": {
				"type": "string",
				"defaultValue": "Sales"
			}
		},
		"folder": {
			"name": "utility"
		},
		"annotations": []
	}
}