{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfmetada012022"
		},
		"ls_adls_data_engineering_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_data_engineering_dl'"
		},
		"ls_asql_dataengineering_metadata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_asql_dataengineering_metadata'"
		},
		"ls_blob_buyalot_webstore_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_buyalot_webstore'"
		},
		"ls_adls_data_engineering_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsmetadata012022.dfs.core.windows.net"
		},
		"trg_ingest_uploaded_files_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/dd32bb1e-bacf-446c-a8f1-3844234c3083/resourceGroups/adf_udemy_metadata_ingestion/providers/Microsoft.Storage/storageAccounts/blobwebstore1801"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfmetada012022_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_data_engineering_dl_accountKey": {
						"value": "[parameters('ls_adls_data_engineering_dl_accountKey')]"
					},
					"ls_asql_dataengineering_metadata_connectionString": {
						"value": "[parameters('ls_asql_dataengineering_metadata_connectionString')]"
					},
					"ls_blob_buyalot_webstore_connectionString": {
						"value": "[parameters('ls_blob_buyalot_webstore_connectionString')]"
					},
					"ls_adls_data_engineering_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_data_engineering_dl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfmetada012022_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trg_ingest_uploaded_files_properties_typeProperties_scope": {
						"value": "[parameters('trg_ingest_uploaded_files_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfmetada012022_ArmTemplate_0')]"
			]
		}
	]
}